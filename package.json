{
  "name": "el-form-renderer-vue3",
  "version": "2.0.10",
  "author": {
    "name": "Mr.Jia",
    "email": "jiagongzi0625@gmail.com"
  },
  "description": "render form-item easily",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiayc4215/form-render.git"
  },
  "type": "module",
  "main": "./dist/el-form-renderer-vue3.umd.js",
  "module": "./dist/el-form-renderer-vue3.es.js",
  "unpkg": "./dist/el-form-renderer-vue3.iife.js",
  "jsdelivr": "./dist/el-form-renderer-vue3.iife.js",
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "vue",
    "element",
    "render",
    "form",
    "component"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/el-form-renderer-vue3.es.js",
      "require": "./dist/el-form-renderer-vue3.cjs.js",
      "default": "./dist/el-form-renderer-vue3.umd.js"
    }
  },
  "scripts": {
    "dev": "initial-scan docs && vitepress dev .docs --host",
    "build": "vite build",
    "build:lib": "pnpm lint && pnpm build",
    "docs:dev": "vitepress dev docs",
    "copy:docs": "node scripts/copy-docs.cjs",
    "docs:build": "initial-scan docs && cross-env NODE_ENV=production && npm run copy:docs && vitepress build .docs",
    "docs:serve": "cross-env NODE_ENV=production vitepress serve .docs --host",
    "lint": "eslint . --ext .js,.mjs,.cjs,.vue --config ./eslint.config.js",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs,.vue --fix --config ./eslint.config.js",
    "lint:prettier": "prettier --write \"**/*.{js,ts,mjs,cjs,json,tsx,css,less,scss,vue,html,md}\"",
    "prepare": "husky",
    "commit": "git-cz",
    "up": "taze major -r -w -I -l",
    "release": "release-it",
    "e2e": "cypress open",
    "test": "jest --verbose"
  },
  "homepage": "https://github.com/jiayc4215/form-render",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.includes": "^4.3.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.kebabcase": "^4.1.1",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.topairs": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@element-plus/icons-vue": "^2.3.2",
    "@eslint/js": "^9.39.2",
    "@ruabick/md-demo-plugins": "^0.3.3",
    "@ruabick/vite-plugin-gen-temp": "^0.3.3",
    "@ruabick/vitepress-demo-block": "^0.3.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "axios": "^1.13.2",
    "babel-jest": "^30.2.0",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cypress": "^15.9.0",
    "cz-git": "^1.12.0",
    "element-plus": "^2.13.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.1",
    "eslint-plugin-vue": "^10.7.0",
    "fs-extra": "^11.3.3",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4",
    "sass": "^1.97.3",
    "taze": "^19.9.2",
    "vite": "^7.3.1",
    "vitepress": "1.6.4",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "element-plus": "^2.4.4"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{js,mjs,cjs,vue}": "eslint --fix",
    "*.{js,mjs,cjs,vue,json,css,less,scss,vue,html,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
